{
    "env": {
        "jasmine": true,
        "node": true,
        "mocha": true,
        "browser": true,
        "builtin": true,
        "es6": true
    },
    "globals": {},
    "rules": {
        "arrow-spacing": 2,
        "arrow-parens": 2,
        "block-spacing": [
            2,
            "always"
        ],
        "brace-style": [
            1,
            "1tbs",
            {
                "allowSingleLine": true
            }
        ],
        "camelcase": 2,
        "comma-style": [
            2,
            "last"
        ],
        "comma-dangle": [
            2,
            "never"
        ],
        "consistent-this": [
            2,
            "self"
        ],
        "curly": [
            2,
            "all"
        ],
        "dot-location": [
            2,
            "property"
        ],
        "dot-notation": [
            2,
            {
                "allowKeywords": true
            }
        ],
        "eol-last": 1,
        "eqeqeq": [
            2,
            "allow-null"
        ],
        "indent": [
            1,
            2
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "one-var": [
            2,
            "never"
        ],
        "max-depth": [
            2,
            3
        ],
        "max-len": [
            2,
            120
        ],
        "max-statements": [
            1,
            16
        ],
        "new-cap": 2,
        "no-caller": 2,
        "no-cond-assign": [
            2,
            "except-parens"
        ],
        "no-const-assign": 2,
        "no-debugger": 1,
        "no-eval": 1,
        "no-extend-native": 1,
        "no-extra-semi": 1,
        "no-iterator": 1,
        "no-irregular-whitespace": 2,
        "no-loop-func": 1,
        "no-lonely-if": 1,
        "no-multi-str": 1,
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": 2,
        "no-proto": 1,
        "no-script-url": 1,
        "no-trailing-spaces": 2,
        "no-undef": 1,
        "no-unneeded-ternary": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "no-with": 2,
        "prefer-const": 2,
        "quote-props": [
            2,
            "as-needed"
        ],
        "quotes": [
            2,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "keyword-spacing": 2,
        "space-before-blocks": [
            2,
            "always"
        ],
        "space-before-function-paren": [
            2,
            {
                "anonymous": "always",
                "named": "never"
            }
        ],
        "space-in-parens": [
            2,
            "never"
        ],
        "valid-typeof": 1
    }
}